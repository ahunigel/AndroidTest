<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionCode="8"
	android:versionName="2.2"
	package="com.hiapk.installer"
	>
	<application
		android:label="@7F040000"
		android:icon="@7F020000"
		>
		<activity
			android:theme="@android:01030006"
			android:name="AppTabActivity"
			android:clearTaskOnLaunch="true"
			android:configChanges="0x000000A0"
			>
			<intent-filter
				>
				<action
					android:name="android.intent.action.MAIN"
					>
				</action>
				<category
					android:name="android.intent.category.LAUNCHER"
					>
				</category>
			</intent-filter>
		</activity>
		<activity
			android:name="AppInstaller"
			>
			<intent-filter
				>
				<action
					android:name="android.intent.action.MAIN"
					>
				</action>
				<category
					android:name="android.intent.category.DEFAULT"
					>
				</category>
			</intent-filter>
		</activity>
		<activity
			android:name="SystemAppAct"
			>
		</activity>
		<activity
			android:name="SyncAppAct"
			>
		</activity>
	</application>
	<uses-sdk
		android:minSdkVersion="3"
		android:targetSdkVersion="8"
		>
	</uses-sdk>
	<uses-permission
		android:name="android.permission.INSTALL_PACKAGES"
		>
	</uses-permission>
	<uses-permission
		android:name="android.permission.CLEAR_APP_CACHE"
		>
	</uses-permission>
	<uses-permission
		android:name="android.permission.DELETE_PACKAGES"
		>
	</uses-permission>
</manifest>
